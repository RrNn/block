  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "cookie-parser": "~1.4.3",
    "db-migrate": "^0.11.5",
    "db-migrate-pg": "^0.4.0",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "express": "~4.16.0",
    "googleapis": "^37.2.0",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.4.0",
    "morgan": "~1.9.0",
    "nock": "^10.0.6",
    "node-pre-gyp": "^0.13.0",
    "nodemailer": "^5.1.1",
    "pg-promise": "^8.5.5",
    "pug": "^2.0.3"
  },
  "engines": {
    "node": "10.4.0"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "mocha": "^6.1.4",
    "supertest": "^4.0.2"
  }
